{
  "name": "Payment",
  "base": "PersistedModel",
  "strict": "validate",
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "memberId":{
      "type":"string",
      "required": true
    },
    "programId":{
      "type":"string",
      "required": true
    },
    "transactionId":{
      "type":"string",
      "required": true
    },
    "paidDate": {
      "type": "date",
      "defaultFn": "now"
    },
    "paidAmount": {
      "type": "number",
      "required": true
    },
    "paymentStatus": {
      "type": "string",
      "default": "pending"
    },
    "ipnValidated":{
      "type":"boolean",
      "default":"false"
    },
    "status":{
      "type":"boolean",
      "default":"true"
    },
    "created": {
      "type": "date",
      "defaultFn": "now"
    },
    "modified": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [
    ],
  "relations": {
    "member": {
      "type": "belongsTo",
      "model": "Member",
      "foreignKey": "memberId"
    },
    "program": {
      "type": "belongsTo",
      "model": "Program",
      "foreignKey": "programId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "accessType": "READ",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "accessType": "WRITE",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
