{
  "name": "Exercise",
  "base": "PersistedModel",
  "strict": "validate",
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description":{
      "type": "string",
      "required": true
    },
    "image":{
      "type": "string"
    },
    "memberId":{
      "type":"string",
      "required": true
    },
    "categoryId":{
      "type":"string",
      "required": true
    },
    "status":{
      "type":"boolean",
      "default":"true"
    },
    "created": {
      "type": "date",
      "defaultFn": "now"
    },
    "modified": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [
    ],
  "relations": {
    "member": {
      "type": "belongsTo",
      "model": "Member",
      "foreignKey": "memberId"
    },
    "category": {
      "type": "belongsTo",
      "model": "Category",
      "foreignKey": "categoryId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "accessType": "READ",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "accessType": "WRITE",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
